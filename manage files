const express = require("express")
const router = express.Router()

const FILE_STORAGE = require("../models/FILE_storage");




router.get("/manage_files",async(req,res)=>{
     try {
        const files = await FILE_STORAGE.find().sort({ serialNumber: 1 });
     res.render("../ejs/manage_files.ejs" , {files}) 
    
        
      } catch (error) {
        res.status(500).json({
          success: false,
          message: "Failed to fetch files",
          error: error.message
        });
      }
})




module.exports = router
